{
	"name": "@younho9/notes",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"postinstall": "patch-package",
		"format": "prettier --write --ignore-path ./node_modules/@younho9/prettier-config/.prettierignore",
		"git:pre-commit": "lint-staged",
		"docs:dev": "concurrently 'yarn generate:data:watch' 'vitepress dev docs'",
		"docs:build": "yarn generate:data && vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"docs:format": "yarn format docs",
		"generate:data": "node scripts/generate-data.mjs",
		"generate:data:watch": "nodemon --watch docs -e md -x 'yarn generate:data'"
	},
	"prettier": "@younho9/prettier-config",
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md,mdx}": [
			"yarn format",
			"case-police"
		]
	},
	"dependencies": {
		"markdown-it-external-links": "^0.0.6",
		"markdown-it-wikilinks": "^1.2.0"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@younho9/prettier-config": "^2.3.1",
		"case-police": "^0.5.9",
		"concurrently": "^7.3.0",
		"d3": "6.7.0",
		"date-fns": "^2.29.2",
		"force-graph": "1.40.5",
		"gray-matter": "^4.0.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"lodash-es": "^4.17.21",
		"nodemon": "^2.0.19",
		"patch-package": "^6.4.7",
		"pluralize": "^8.0.0",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.1.8",
		"ts-extras": "^0.11.0",
		"typescript": "^4.7.4",
		"vitepress": "^1.0.0-alpha.12",
		"vue": "^3.2.37"
	}
}

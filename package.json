{
	"name": "notes.younho9.com",
	"private": true,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"format": "prettier --write --ignore-path ./node_modules/@younho9/prettier-config/.prettierignore",
		"git:pre-commit": "lint-staged",
		"docs:dev": "yarn generate:data && vitepress dev docs",
		"docs:build": "yarn generate:data && vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"docs:format": "yarn format docs",
		"docs:update-frontmatter": "node scripts/update-frontmatter.mjs",
		"generate:data": "node scripts/generate-data.mjs",
		"generate:data:watch": "nodemon --watch docs -e md -x 'yarn generate:data'"
	},
	"prettier": "@younho9/prettier-config",
	"lint-staged": {
		"!(**/.obsidian/**)*.{js,jsx,ts,tsx,json,css,md,mdx}": [
			"yarn format",
			"case-police"
		],
		"docs/notes/*.md": [
			"yarn docs:update-frontmatter"
		]
	},
	"devDependencies": {
		"@carbon/icons-vue": "^10.79.1",
		"@sindresorhus/is": "^5.6.0",
		"@types/d3": "^7.4.3",
		"@types/force-graph": "^1.26.1",
		"@types/lodash-es": "^4.17.12",
		"@types/markdown-it": "^12.2.3",
		"@types/markdown-it-external-links": "^0.0.3",
		"@types/node": "^18.19.7",
		"@vueuse/core": "^9.13.0",
		"@younho9/prettier-config": "^2.4.0",
		"case-police": "^0.5.14",
		"d3": "6.7.0",
		"date-fns": "^2.30.0",
		"force-graph": "1.40.5",
		"fuse.js": "^6.6.2",
		"gray-matter": "^4.0.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"lodash-es": "^4.17.21",
		"markdown-it": "^13.0.2",
		"markdown-it-external-links": "^0.0.6",
		"markdown-it-footnote": "^3.0.3",
		"markdown-it-hashtag": "^0.4.0",
		"markdown-it-regexp": "^0.4.0",
		"markdown-it-task-lists": "^2.1.1",
		"markdown-it-wikilinks": "^1.4.0",
		"nodemon": "^2.0.22",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.8.8",
		"sanitize-filename": "^1.6.3",
		"typescript": "^4.9.5",
		"unocss": "^0.58.3",
		"vitepress": "1.0.0-rc.36",
		"vitepress-plugin-twoslash": "0.10.0-beta.1",
		"vue": "^3.4.13",
		"vue-spinner": "^1.0.4"
	},
	"pnpm": {
		"patchedDependencies": {
			"vitepress@1.0.0-rc.36": "patches/vitepress@1.0.0-rc.36.patch"
		}
	}
}
